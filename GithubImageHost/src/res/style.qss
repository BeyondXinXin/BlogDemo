/*---------------------------------------------------------------------------*/
/*  QSpinBox  */

QSpinBox{
color: rgb(192, 192, 192);
border:1px solid #67696B;
background-color: rgb(65, 65, 69);
border-color: rgb(110, 110, 110);
}

QSpinBox:focus,
QSpinBox:hover{
border:1px solid #67696B;
}

QSpinBox::up-button{
image:url(:/StyleResources/add_top.png);
width:10px;
height:10px;
padding:2px 5px 0px 0px;
}

QSpinBox::down-button{
image:url(:/StyleResources/add_bottom.png);
width:10px;
height:10px;
padding:0px 5px 2px 0px;
}

QSpinBox::up-button:pressed{
top:-2px;
}

QSpinBox::down-button:pressed{
bottom:-2px;
}


/*  QFrame/Line  */


/*---------------------------------------------------------------------------*/
/*  QGroupBox  */

QGroupBox{
color: white;
border:1px solid #67696B;
border-radius:5px;
margin-top:3ex;
}

QGroupBox::title{
subcontrol-origin:margin;
position:relative;
left:10px;
}

QGroupBox::indicator{
padding:0px -3px 0px 0px;
}

QGroupBox::indicatorr{
width:13px;
height:13px;
}

QGroupBox::indicator:unchecked{
image:url(:/StyleResources/checkbox_unchecked.png);
}

QGroupBox::indicator:unchecked:disabled{
image:url(:/StyleResources/checkbox_unchecked_disable.png);
}

QGroupBox::indicator:checked{
image:url(:/StyleResources/checkbox_checked.png);
}

QGroupBox::indicator:checked:disabled{
image:url(:/StyleResources/checkbox_checked_disable.png);
}

QGroupBox::indicator:indeterminate{
image:url(:/StyleResources/checkbox_parcial.png);
}

QGroupBox::indicator:indeterminate:disabled{
image:url(:/StyleResources/checkbox_parcial_disable.png);
}

/*---------------------------------------------------------------------------*/

QPushButton{
text-align: center;
font-size:12px;
}

QPushButton:hover{
border:1px solid #2ca6e0;
}

/*  QPushButton  */
QPushButton[CustomStyle="transparent_blue"]{
background-color:transparent;
border:1px solid rgba(39, 141, 255, 1);
color:rgba(39, 141, 255, 1);
}

QPushButton:hover[CustomStyle="transparent_blue"]{
background-color:rgba(39, 141, 255, 1);
border:1px solid #2ca6e0;
color:rgba(255, 255, 255, 1);
}

QPushButton[CustomStyle="transparent_green"]{
background-color:rgba(34, 51, 15, 1);
border:1px solid rgba(34, 51, 15, 1);
color:rgba(121, 203, 80, 1);
}

QPushButton:hover[CustomStyle="transparent_green"]{
background-color:rgba(82, 155, 46, 1);
border:1px solid #2ca6e0;
color:rgba(232, 230, 227, 1);
}

QPushButton[CustomStyle="transparent"]{
border:none;
background:transparent;
}

QPushButton:hover[CustomStyle="transparent"]{
background-color:rgba(64, 64, 64, 1);
}


/*---------------------------------------------------------------------------*/
/*  QToolButton  */

QToolButton{
background:rgba(85, 93, 97, 1);
}

QToolButton:hover{
background:qlineargradient(
spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #262829,stop:1 #262829);
}

QToolButton:pressed{
background:qlineargradient(
spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #404244,stop:1 #404244);
}

QToolButton::menu-indicator{
image:None;
}


QToolButton[CustomStyle="ControlBtn"]{
border:none;
background-color:rgb(24, 26, 27);
}

QToolButton[CustomStyle="ControlBtn"]::checked{
border:none;
background-color:rgb(32, 35, 37);

}





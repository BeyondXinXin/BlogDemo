/*---------------------------------------------------------------------------*/
/*  QMenu  */

QMenu,
QMenu:disabled{
color:#BEC0C2;
background-color:#404244;
border:1px solid #67696B;
margin:0px;
}

QMenu::item:disabled{
background:#2E2F30;
border-color:#404244;
color:#67696B;
}

QMenu::item{
padding:3px 20px;
}

QMenu::indicator{
width:13px;
height:13px;
}

QMenu::item:selected{
color:#BEC0C2;
border:0px solid #67696B;
background:#262829;
}

QMenu::separator{
height:1px;
background:#67696B;
}

/*---------------------------------------------------------------------------*/
/*  QMenuBar  */

QMenuBar,
QMenuBar:disabled{
color:#BEC0C2;
background:transparent;
border: 0px solid gray;
margin:0px;
}

QMenuBar::item{
color:#BEC0C2;
background-color:#404244;
margin:0px;
padding:3px 10px;
background:transparent;
border: 0px solid gray;
}

QMenuBar::item:selected{
color:#BEC0C2;
border:0px solid #67696B;
background:#262829;
background-color: rgb(65, 65, 69);
}

/*---------------------------------------------------------------------------*/
/*  QMenuBar  QToolBar  */

QToolBar{
border-left: 8px solid transparent;
border-right: 0px solid transparent;
border-top: 0px solid transparent;
border-bottom: 0px solid transparent;
spacing: 25px;
}

QToolBar::separator{
background-color: rgb(25, 25, 25);
width: 2px;height: 8px;
}

QToolBar>QToolButton{
background:transparent;
border: 0px solid gray;
}

QToolBar>QToolButton::hover{
background-color: rgb(65, 65, 69);
}


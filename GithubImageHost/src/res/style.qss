/*---------------------------------------------------------------------------*/
/*  QWidget  QTableWidget */

QWidget{
font-size:18px;
font-family: "Microsoft Yahei";
background-color:rgb(24, 26, 27);
}

QWidget#MainWindow,
QWidget#main_wid{
background-color:rgb(32, 35, 37);
}

QTableWidget{
background-color:rgb(53, 58, 61);
}

/*---------------------------------------------------------------------------*/
/*  QLineEdit  */

QLineEdit{
background:#2E2F30;
border:1px solid #67696B;
border-radius:3px;
padding:2px;
selection-background-color:#00BB9E;
selection-color:#FFFFFF;
/*  background:transparent;  */
}

QLineEdit:focus,
QLineEdit:hover{
border:1px solid #67696B;
}

/*---------------------------------------------------------------------------*/
/*  QTextEdit  */

QTextEdit{
border:none;
border-radius:5px;
padding:2px;
selection-background-color:#00BB9E;
selection-color:#FFFFFF;
font-size:18px;
color:rgba(149,162,171,1);
background:transparent;
}


QTextEdit:focus
QTextEdit:hover{
border:1px solid #67696B;
}


/*---------------------------------------------------------------------------*/
